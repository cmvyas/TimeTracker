{
    "manifest_version": 3,
    "name": "Time Tracker Extension",
    "version": "1.0",
    "description": "Track time and manage weekly reports.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
  }
  